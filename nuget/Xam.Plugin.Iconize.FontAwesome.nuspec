<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Xam.Plugin.Iconize.FontAwesome</id>
    <version>$version$</version>
    <title>Iconize Plugin for Xamarin.Forms - Font Awesome</title>
    <authors>Jeremy Marcus</authors>
    <owners>Jeremy Marcus</owners>
    <licenseUrl>https://github.com/jsmarcus/Iconize/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/jsmarcus/Iconize</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Easily add icon fonts to your Xamarin.Forms applications!</description>
    <summary>Iconize Plugin for Xamarin.Forms - Font Awesome</summary>
    <copyright>Copyright Â© 2017</copyright>
    <tags>xamarin, pcl, plugin, xamarin.forms, android, ios, windows</tags>
    <dependencies>
      <group>
        <dependency id="Xam.Plugin.Iconize" version="$version$" />
      </group>
    </dependencies>
    <references>
      <!--netstandard-->
      <group targetFramework="netstandard2.0">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
      </group>

      <!--Android-->
      <group targetFramework="MonoAndroid10">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
	      <reference file="Plugin.Iconize.FontAwesome.Android.dll" />
      </group>

      <!--iOS-->
      <group targetFramework="Xamarin.iOS10">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
	      <reference file="Plugin.Iconize.FontAwesome.iOS.dll" />
      </group>

      <!--UWP-->
      <group targetFramework="uap10.0">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
	      <reference file="Plugin.Iconize.FontAwesome.UWP.dll" />
      </group>
    </references>
    <contentFiles>
      <!--iOS-->
			<files include="any/Xamarin.iOS10/iconize-fontawesome-brands.otf" buildAction="BundleResource" copyToOutput="true" flatten="true" />
	    <files include="any/Xamarin.iOS10/iconize-fontawesome-regular.otf" buildAction="BundleResource" copyToOutput="true" flatten="true" />
	    <files include="any/Xamarin.iOS10/iconize-fontawesome-solid.otf" buildAction="BundleResource" copyToOutput="true" flatten="true" />
    </contentFiles>
  </metadata>
  <files>
    <!--netstandard-->
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\netstandard2.0\Plugin.Iconize.FontAwesome.dll" target="lib\netstandard2.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\netstandard2.0\Plugin.Iconize.FontAwesome.*pdb" target="lib\netstandard2.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\netstandard2.0\Plugin.Iconize.FontAwesome.*xml" target="lib\netstandard2.0" />

    <!--Android-->
