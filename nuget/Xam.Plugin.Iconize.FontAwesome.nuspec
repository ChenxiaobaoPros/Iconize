    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.*pdb" target="lib\MonoAndroid10" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.*xml" target="lib\MonoAndroid10" />
