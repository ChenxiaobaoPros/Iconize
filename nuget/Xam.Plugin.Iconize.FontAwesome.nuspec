	  <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.UWP\bin\$configuration$\Plugin.Iconize.FontAwesome.UWP.*xml" target="lib\uap10.0" />
    <file src="src\Fonts\FontAwesome\iconize-fontawesome-brands.otf" target="lib\uap10.0\Plugin.Iconize.FontAwesome" />
	  <file src="src\Fonts\FontAwesome\iconize-fontawesome-regular.otf" target="lib\uap10.0\Plugin.Iconize.FontAwesome" />
	  <file src="src\Fonts\FontAwesome\iconize-fontawesome-solid.otf" target="lib\uap10.0\Plugin.Iconize.FontAwesome" />
