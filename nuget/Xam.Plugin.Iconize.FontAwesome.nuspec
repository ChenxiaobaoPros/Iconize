	  <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.Android\bin\$configuration$\Plugin.Iconize.FontAwesome.Android.*xml" target="lib\MonoAndroid10" />

    <!--iOS-->
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.dll" target="lib\Xamarin.iOS10" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.*pdb" target="lib\Xamarin.iOS10" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.*xml" target="lib\Xamarin.iOS10" />
	  <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.iOS\bin\$configuration$\Plugin.Iconize.FontAwesome.iOS.dll" target="lib\Xamarin.iOS10" />
	  <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.iOS\bin\$configuration$\Plugin.Iconize.FontAwesome.iOS.*pdb" target="lib\Xamarin.iOS10" />
	  <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.iOS\bin\$configuration$\Plugin.Iconize.FontAwesome.iOS.*xml" target="lib\Xamarin.iOS10" />
    <file src="src\Fonts\FontAwesome\iconize-fontawesome-brands.otf" target="contentFiles\any\Xamarin.iOS10" />
	  <file src="src\Fonts\FontAwesome\iconize-fontawesome-regular.otf" target="contentFiles\any\Xamarin.iOS10" />
	  <file src="src\Fonts\FontAwesome\iconize-fontawesome-solid.otf" target="contentFiles\any\Xamarin.iOS10" />

    <!--UWP-->
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.dll" target="lib\uap10.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.*pdb" target="lib\uap10.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\Plugin.Iconize.FontAwesome.*xml" target="lib\uap10.0" />
