<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Xam.Plugin.Iconize.FontAwesome</id>
    <version>$version$</version>
    <title>Iconize Plugin for Xamarin.Forms - Font Awesome</title>
    <authors>Jeremy Marcus</authors>
    <owners>Jeremy Marcus</owners>
    <licenseUrl>https://github.com/jsmarcus/Iconize/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/jsmarcus/Iconize</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Easily add icon fonts to your Xamarin.Forms applications!</description>
    <summary>Iconize Plugin for Xamarin.Forms - Font Awesome</summary>
    <copyright>Copyright Â© 2017</copyright>
    <tags>xamarin, pcl, plugin, xamarin.forms, android, ios, windows</tags>
    <dependencies>
      <group>
        <dependency id="Xam.Plugin.Iconize" version="$version$" />
      </group>
    </dependencies>
    <references>
      <!--netstandard-->
      <group targetFramework="netstandard1.0">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
      </group>

      <!--Android-->
      <group targetFramework="MonoAndroid10">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
      </group>

      <!--iOS-->
      <group targetFramework="Xamarin.iOS10">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
      </group>

      <!--UWP-->
      <group targetFramework="uap10.0">
        <reference file="Plugin.Iconize.FontAwesome.dll" />
      </group>
    </references>
    <contentFiles>
      <!--iOS-->
      <files include="any/xamarinios/iconize-fontawesome.ttf" buildAction="BundleResource" copyToOutput="true" flatten="true" />
    </contentFiles>
  </metadata>
  <files>
    <!--netstandard-->
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\netstandard1.0\Plugin.Iconize.FontAwesome.dll" target="lib\netstandard1.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\netstandard1.0\Plugin.Iconize.FontAwesome.*pdb" target="lib\netstandard1.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome\bin\$configuration$\netstandard1.0\Plugin.Iconize.FontAwesome.*xml" target="lib\netstandard1.0" />

    <!--Android-->
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.Android\bin\$configuration$\Plugin.Iconize.FontAwesome.dll" target="lib\MonoAndroid10" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.Android\bin\$configuration$\Plugin.Iconize.FontAwesome.*pdb" target="lib\MonoAndroid10" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.Android\bin\$configuration$\Plugin.Iconize.FontAwesome.*xml" target="lib\MonoAndroid10" />

    <!--iOS-->
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.iOS\bin\$configuration$\Plugin.Iconize.FontAwesome.dll" target="lib\Xamarin.iOS10" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.iOS\bin\$configuration$\Plugin.Iconize.FontAwesome.*pdb" target="lib\Xamarin.iOS10" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.iOS\bin\$configuration$\Plugin.Iconize.FontAwesome.*xml" target="lib\Xamarin.iOS10" />
    <file src="src\Fonts\FontAwesome\iconize-fontawesome.ttf" target="Content" />

    <!--UWP-->
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.UWP\bin\$configuration$\Plugin.Iconize.FontAwesome.dll" target="lib\uap10.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.UWP\bin\$configuration$\Plugin.Iconize.FontAwesome.*pdb" target="lib\uap10.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.UWP\bin\$configuration$\Plugin.Iconize.FontAwesome.*pri" target="lib\uap10.0" />
    <file src="src\Fonts\FontAwesome\Plugin.Iconize.FontAwesome.UWP\bin\$configuration$\Plugin.Iconize.FontAwesome.*xml" target="lib\uap10.0" />
    <file src="src\Fonts\FontAwesome\iconize-fontawesome.ttf" target="lib\uap10.0\Plugin.Iconize.FontAwesome" />
  </files>
</package>