<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Xam.Plugin.Iconize</id>
    <version>$version$</version>
    <title>Iconize Plugin for Xamarin.Forms</title>
    <authors>Jeremy Marcus</authors>
    <owners>Jeremy Marcus</owners>
    <licenseUrl>https://github.com/jsmarcus/Iconize/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/jsmarcus/Iconize</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Easily add icon fonts to your Xamarin.Forms applications!</description>
    <summary>Iconize Plugin for Xamarin.Forms</summary>
    <copyright>Copyright Â© 2017</copyright>
    <tags>xamarin, pcl, plugin, xamarin.forms, android, ios, windows</tags>
    <dependencies>
      <group>
        <dependency id="Xamarin.Forms" version="[2.3.5, 3)" />
      </group>

      <group targetFramework="uap10.0">
        <dependency id="SkiaSharp" version="[1.58, 2)" />
        <dependency id="Xamarin.Forms" version="[2.3.5, 3)" />
      </group>
    </dependencies>
    <references>
      <!--netstandard-->
      <group targetFramework="netstandard1.0">
        <reference file="Plugin.Iconize.dll" />
      </group>

      <!--Android-->
      <group targetFramework="MonoAndroid10">
        <reference file="Plugin.Iconize.dll" />
      </group>

      <!--iOS-->
      <group targetFramework="Xamarin.iOS10">
        <reference file="Plugin.Iconize.dll" />
      </group>

      <!--UWP-->
      <group targetFramework="uap10.0">
        <reference file="Plugin.Iconize.dll" />
      </group>
    </references>
  </metadata>
  <files>
    <!--netstandard-->
    <file src="src\Plugin.Iconize\bin\$configuration$\netstandard1.0\Plugin.Iconize.dll" target="lib\netstandard1.0" />
    <file src="src\Plugin.Iconize\bin\$configuration$\netstandard1.0\Plugin.Iconize.*pdb" target="lib\netstandard1.0" />
    <file src="src\Plugin.Iconize\bin\$configuration$\netstandard1.0\Plugin.Iconize.*xml" target="lib\netstandard1.0" />

    <!--Android-->
    <file src="src\Plugin.Iconize.Android\bin\$configuration$\Plugin.Iconize.dll" target="lib\MonoAndroid10" />
    <file src="src\Plugin.Iconize.Android\bin\$configuration$\Plugin.Iconize.*pdb" target="lib\MonoAndroid10" />
    <file src="src\Plugin.Iconize.Android\bin\$configuration$\Plugin.Iconize.*xml" target="lib\MonoAndroid10" />

    <!--iOS-->
    <file src="src\Plugin.Iconize.iOS\bin\$configuration$\Plugin.Iconize.dll" target="lib\Xamarin.iOS10" />
    <file src="src\Plugin.Iconize.iOS\bin\$configuration$\Plugin.Iconize.*pdb" target="lib\Xamarin.iOS10" />
    <file src="src\Plugin.Iconize.iOS\bin\$configuration$\Plugin.Iconize.*xml" target="lib\Xamarin.iOS10" />

    <!--UWP-->
    <file src="src\Plugin.Iconize.UWP\bin\$configuration$\Plugin.Iconize.dll" target="lib\uap10.0" />
    <file src="src\Plugin.Iconize.UWP\bin\$configuration$\Plugin.Iconize.*pdb" target="lib\uap10.0" />
    <file src="src\Plugin.Iconize.UWP\bin\$configuration$\Plugin.Iconize.*pri" target="lib\uap10.0" />
    <file src="src\Plugin.Iconize.UWP\bin\$configuration$\Plugin.Iconize.*xml" target="lib\uap10.0" />
  </files>
</package>